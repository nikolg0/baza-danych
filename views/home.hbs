<h1 class="text-center">Lista użytkowników</h1>
<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th scope="col">Imię i nazwisko</th>
      <th scope="col">Adres</th>
      <th scope="col">Strona WWW</th>
    </tr>
  </thead>

  <tbody>
    {{#each users}}
      <tr>
        <td>{{name}}</td>
        <td>{{address.street}}
          {{address.suite}}
          {{address.city}}
          {{address.zipcode}}</td>
        <td><a href="http://{{website}}">{{website}}</a></td>
      </tr>
    {{/each}}
  </tbody>
</table>